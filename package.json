{
  "name": "mooti",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "android-prod": "react-native run-android --variant=release",
    "ios": "npx react-native run-ios --simulator \"iPhone 14\"",
    "ipad": "npx react-native run-ios --device \"iPad Gian\"",
    "iphone": "npx react-native run-ios --device \"iPhone Gian\"",
    "link-font": "npx react-native-asset",
    "start": "npx react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean": "cd android && ./gradlew clean",
    "pod": "rm -rf ios/Pods ios/Podfile.lock && cd ios && pod install",
    "releaseApk": "cd android && ./gradlew assembleRelease && open app/build/outputs/apk/release",
    "bundleRelease": "cd android && ./gradlew clean && ./gradlew bundleRelease && open app/build/outputs/bundle/release",
    "reinstall": "rm -rf node_modules && yarn",
    "fixDebug": "rm -rf node_modules/react-native/Libraries/BatchedBridge/MessageQueue.js && cp -R __tests__/MessageQueue.js node_modules/react-native/Libraries/BatchedBridge/",
    "fixGif": "rm -rf node_modules/react-native-fast-image/RNFastImage.podspec && cp -R __tests__/RNFastImage.podspec node_modules/react-native-fast-image/",
    "getHash": "keytool -exportcert -alias mootie-alias -keystore android/app/mootie.keystore | openssl sha1 -binary | openssl base64",
    "postinstall": "react-native setup-ios-permissions && pod-install && npx patch-package"
  },
  "dependencies": {
    "@brigad/react-native-skadnetwork": "^0.1.2",
